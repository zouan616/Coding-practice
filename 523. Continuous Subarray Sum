class Solution {
public:
    bool checkSubarraySum(vector<int>& nums, int k) {
        int sum;
        for(int i=0; i < nums.size(); i++)
        {
            sum = 0;
            for(int j = i; j < nums.size(); j++)
            {
                sum = sum + nums[j];
                if((sum%k==0)&&(j>i))
                {
                    return true;
                }
            }
        }
    return false;        
    }
};
